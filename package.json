{
  "name": "Borealis",
  "version": "0.0.1DEV",
  "description": "Best TODO app yet",
  "repository": "https://github.com/JustMaffie/Borealis",
  "homepage": "",
  "author": {
    "name": "Jori van Ee",
    "email": "jori@justmaffie.nl"
  },
  "scripts": {
    "dev": "concurrently \"set BROWSER=none&&yarn start\" \"wait-on http://localhost:3000 && electron . --dev\"",
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "package": "yarn build && build --publish never"
  },
  "main": "main.js",
  "license": "AGPL",
  "private": true,
  "devDependencies": {
    "autoprefixer": "8.2.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.2.2",
    "babel-jest": "22.4.3",
    "babel-loader": "7.1.4",
    "babel-preset-react-app": "^3.1.1",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.2",
    "chalk": "2.3.2",
    "concurrently": "^3.5.1",
    "css-loader": "0.28.11",
    "dotenv": "5.0.1",
    "dotenv-expand": "4.2.0",
    "electron-builder": "^20.8.1",
    "electron-devtools-installer": "^2.2.3",
    "eslint": "4.19.1",
    "eslint-config-react-app": "^2.1.0",
    "eslint-loader": "2.0.0",
    "eslint-plugin-flowtype": "2.46.1",
    "eslint-plugin-import": "2.11.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.7.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.11",
    "fs-extra": "5.0.0",
    "html-webpack-plugin": "3.2.0",
    "jest": "22.4.3",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.3.0",
    "postcss-loader": "2.1.3",
    "promise": "8.0.1",
    "raf": "3.4.0",
    "react": "^16.3.1",
    "react-dev-utils": "^5.0.1",
    "react-dom": "^16.3.1",
    "resolve": "1.7.1",
    "style-loader": "0.20.3",
    "sw-precache-webpack-plugin": "0.11.5",
    "url-loader": "1.0.1",
    "wait-on": "^2.1.0",
    "webpack": "4.5.0",
    "webpack-dev-server": "3.1.3",
    "webpack-manifest-plugin": "2.0.0",
    "whatwg-fetch": "2.0.4"
  },
  "dependencies": {
    "devtron": "^1.4.0",
    "electron-debug": "^1.5.0",
    "font-awesome": "^4.7.0",
    "history": "^4.7.2",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-hot-loader": "^4.0.1",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^4.0.8",
    "redux": "^3.7.2",
    "semantic-ui-react": "^0.79.1"
  },
  "build": {
    "appId": "nl.JustMaffie.Borealis",
    "productName": "Borealis",
    "copyright": "Copyright Â© 2018 JustMaffie",
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "build/favicon.ico",
      "publisherName": "JustMaffie"
    },
    "portable": {
      "requestExecutionLevel": "admin"
    },
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "package.json",
      "main.js",
      "LICENSE"
    ],
    "nsis": {
      "oneClick": true,
      "perMachine": true,
      "installerIcon": "build/favicon.ico",
      "uninstallerIcon": "build/favicon.ico",
      "license": "LICENSE"
    },
    "directories": {
      "buildResources": "assets"
    }
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
